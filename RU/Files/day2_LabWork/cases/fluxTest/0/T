FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293.15;

boundaryField
{
    inlet
    {
        type            conductiveHeatFlux;
        lambdaWall	0.1;
        q		100;
        value		uniform 293.15;
    }

    walls
    {
        type            zeroGradient;
    }

    outlet
    {
	type		fixedValue;
	value		uniform 293.15;
    }
}

