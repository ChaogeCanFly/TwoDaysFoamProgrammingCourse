FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        tolerance       1e-06;
        relTol          0.01;
        preconditioner	DIC;
    }

    pFinal
    {
        solver          PCG;
        tolerance       1e-06;
        relTol          0;
        preconditioner	DIC;
    }

    "(rho|U|k|omega|epsilon|T|C0|C1|C2)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-07;
        relTol          0;
    }

    "(rho|U|k|omega|epsilon|T|C0|C1|C2)Final"
    {
        $U;
        tolerance       1e-07;
        relTol          0;
    }
}

PISO
{
    nOuterCorrectors 2;
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
}

relaxationFactors
{
}


